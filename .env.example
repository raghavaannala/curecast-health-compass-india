# Dr.Curecast Environment Variables Template
# Copy this file to .env and fill in your actual values

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id

# Google Services
REACT_APP_GOOGLE_TRANSLATE_API_KEY=your_google_translate_api_key_here

# Government Health APIs
REACT_APP_COWIN_API_KEY=your_cowin_api_key_here
REACT_APP_AYUSHMAN_API_KEY=your_ayushman_bharat_api_key_here
REACT_APP_STATE_PORTAL_API_KEY=your_state_health_portal_api_key_here
REACT_APP_IHIP_API_KEY=your_ihip_api_key_here

# Communication Services
REACT_APP_WHATSAPP_ACCESS_TOKEN=your_whatsapp_business_api_token_here
REACT_APP_WHATSAPP_PHONE_NUMBER_ID=your_whatsapp_phone_number_id_here
REACT_APP_SMS_API_KEY=your_sms_gateway_api_key_here
REACT_APP_SMS_SENDER_ID=your_sms_sender_id_here

# Real-time Health Alerts WebSocket
REACT_APP_HEALTH_ALERTS_WS_URL=wss://health-alerts.gov.in/ws/

# Government Health Database APIs
REACT_APP_COWIN_API_KEY=your_cowin_api_key
REACT_APP_AYUSHMAN_API_KEY=your_ayushman_bharat_api_key
REACT_APP_STATE_PORTAL_API_KEY=your_state_health_portal_api_key

# Voice Services (Optional - for enhanced voice features)
REACT_APP_SPEECH_API_KEY=your_speech_service_api_key

# Security & Monitoring
REACT_APP_ENCRYPTION_KEY=your_encryption_key_for_health_data
REACT_APP_MONITORING_API_KEY=your_monitoring_service_key

# Development Settings
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=info
